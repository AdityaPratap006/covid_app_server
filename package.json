  "license": "ISC",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase-admin": "^8.10.0",
    "node-fetch": "^2.6.0",
    "nodemon": "^2.0.3",
    "rimraf": "^3.0.2",
    "@types/express": "^4.17.6",
    "@types/node-fetch": "^2.5.6",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "ts-node-dev": "^1.0.0-pre.44"
  }
}
