{
  "name": "covid_app_server",
  "version": "1.0.0",
  "description": "The backend server for a flutter based covid19 tracking app",
  "main": "./src/server.ts",
  "scripts": {
    "tsc": "tsc",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/server.ts",
